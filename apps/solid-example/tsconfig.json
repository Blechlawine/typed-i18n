{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "jsxImportSource": "solid-js",
        "jsx": "preserve",
        "strict": true,
        "types": ["solid-start/env", "@typed-i18n/solid"],
        "baseUrl": "./",
        "paths": {
            "~/*": ["./src/*"]
        }
    }
}
